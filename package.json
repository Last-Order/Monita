{
  "name": "monita",
  "version": "1.2.9",
  "author": {
    "name": "Eridanus Sora",
    "email": "sora@sound.moe"
  },
  "homepage": "https://github.com/Last-Order/Monita",
  "description": "A Multi-channel Live Stream Viewer",
  "repository": {
    "url": "https://github.com/Last-Order/Monita"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service electron:serve",
    "electron:build-win": "vue-cli-service electron:build --win",
    "electron:build-linux": "vue-cli-service electron:build --linux",
    "electron:build-macos": "vue-cli-service electron:build --mac",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/logo.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "release": "electron-builder --publish onTagOrDraft"
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^0.18.0",
    "flv.js": "^1.5.0",
    "hls.js": "^0.12.3",
    "vue": "^2.6.6",
    "vue-grid-layout": "^2.3.4",
    "vuetify": "^1.5.5",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "css-loader": "^2.1.1",
    "electron": "^4.0.0",
    "electron-icon-builder": "^1.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "material-design-icons-iconfont": "^4.0.5",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-electron-builder": "^1.1.3",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
